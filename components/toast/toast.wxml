<view class="toast" wx:if='{{toastData.showToast}}'>
  <view class="toastContent">
    <view class="container">
      <view class="title">羁绊效果</view>
      <view class="desc">  
        {{toastFetter}}({{toastCount}}):{{desc}}
      </view>
      <view bindtap="close" class="closeIconBox">
        <image src="./close.png" class="closeIcon"/>
      </view>
    </view>
  </view>
</view>